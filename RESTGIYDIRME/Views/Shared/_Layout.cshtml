<!doctype html>
<html class="no-js" lang="en">
<head>
    <meta charset="utf-8">
    <title>{re}start | clean, business oriented HTML template</title>
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    
    @await Html.PartialAsync("/Views/Partials/_CssPartial.cshtml")
</head>
<body class="collapsing_header">

    @await Html.PartialAsync("/Views/Partials/_HeaderPartial.cshtml")

    <div class="main">

        @RenderBody()

        @await Html.PartialAsync("/Views/Partials/_FooterPartial.cshtml")
    </div>
    

    @await Html.PartialAsync("/Views/Partials/_ScriptPartial.cshtml")
</body>
</html>